<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Proiect" transaction-type="RESOURCE_LOCAL">
		<class>pachetProiect.Comanda</class>
		<class>pachetProiect.Document</class>
		<class>pachetProiect.Furnizor</class>
		<class>pachetProiect.FurnizorPlata</class>
		<class>pachetProiect.Gestiune</class>
		<class>pachetProiect.Judet</class>
		<class>pachetProiect.LinieComanda</class>
		<class>pachetProiect.LinieDocument</class>
		<class>pachetProiect.Localitate</class>
		<class>pachetProiect.Necesar</class>
		<class>pachetProiect.Plata</class>
		<class>pachetProiect.Produs</class>
		<class>pachetProiect.Receptie</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5433/postgres"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="postgres"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
